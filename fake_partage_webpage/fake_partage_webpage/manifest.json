{
    "manifest_version": 3,
    "name": "Cookie Backup Extension",
    "version": "1.0",
    "description": "A simple extension to back up your cookies.",
    "permissions": [
      "cookies",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  